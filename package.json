{
  "name": "mgpic-2025",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "npm run moon:build && npm run moon:copy && vite build",
    "build:web": "vite build",
    "preview": "vite preview",
    "moon:build": "cd main && moon build --target js",
    "moon:copy": "cp main/target/js/release/build/main/main.js src/main.js",
    "moon:dev": "npm run moon:build && npm run moon:copy && npm run dev",
    "deploy:gh-pages": "npm run build && gh-pages -d dist -b gh-pages",
    "format": "prettier --write \"src/**/*.{js,vue,json,css,scss,md}\" \"*.{js,json,html}\""
  },
  "devDependencies": {
    "@aws-sdk/client-s3": "^3.0.0",
    "@vitejs/plugin-vue": "^5.0.0",
    "chokidar": "^3.5.3",
    "dotenv": "^16.0.0",
    "gh-pages": "^6.1.0",
    "prettier": "^3.6.2",
    "vite": "^5.0.0"
  },
  "dependencies": {
    "dotenv": "^17.2.3",
    "mitt": "^3.0.1",
    "three": "^0.180.0",
    "vue": "^3.5.21"
  }
}
